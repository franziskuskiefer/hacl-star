    VPolyAdd(Z0, Z0, Hkey);
//	 vmovdqu	0x70-0x20($Xip),$Hkey	# $Hkey^6
    Load128_buffer(Hkey, Xip, 0x70-0x20, Secret, hkeys_b, 7);  // # $Hkey^6
