        $$Z0 == old($$Z0) +. (lo(old($$Ii)) *. lo(old($$T1)));
        $$Z1 == hi(old($$Ii)) *. lo(old($$T1));
        $$Z2 == (old($$Z2) +. old($$T2)) +. old($$Hkey);
        $$Z3 == old($$Z3) +. old($$Z1);

step3:
