        buffer_modifies_specific128(scratch_b, old(mem), mem, 1, 7);
        buffer_modifies_specific128(out_b, old(mem), mem, count*6, #nat(count*6+old(len)-1));
        buffer_modifies_specific128(iv_b,    old(mem), mem, 0, 0);

