        let s7 := $$old(buffer128_read(scratch_b, 7, mem));
        $$T1 == hi(old($$Ii)) *. lo(old($$T2));
        $$T2 == hi(old($$Ii)) *. hi(old($$T2));
        Hkey == h7;
        $$Z0 == old($$Z0) +. (lo(old($$Ii)) *. lo(old($$T2)));
        $$Z1 == lo(old($$Ii)) *. hi(old($$T2));
        $$Z2 == (old($$Z2) +. old($$Hkey)) +. old($$Z1);
        $$Z3 == old($$Z3) +. old($$T1);
