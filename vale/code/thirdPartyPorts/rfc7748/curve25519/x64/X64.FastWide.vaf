procedure{:quick}{:public}{:exportSpecs} fsqr(
    ghost tmp_b:buffer64,
    ghost inA_b:buffer64,
    ghost dst_b:buffer64)
    lets
        tmp_ptr @= rdi; inA_ptr @= rsi; dst_ptr @= rdx; 
        tmp_in := rdi;
        inA_in := rsi;
        dst_in := rdx;
    reads memTaint;
    modifies
        rax; rbx; rcx; rdx; rdi; rsi; r8; r9; r10; r11; r12; r13; r14; r15;         
        efl; mem;    
    requires
        adx_enabled && bmi2_enabled;
        buffers_disjoint(dst_b, inA_b) || dst_b == inA_b;
        buffers_disjoint(dst_b, tmp_b) || dst_b == tmp_b;

        buffers_disjoint(tmp_b, inA_b);

        validDstAddrs64(mem, dst_in, dst_b, 4, memTaint, Secret);
        validSrcAddrs64(mem, inA_in, inA_b, 4, memTaint, Secret);
        validDstAddrs64(mem, tmp_in, tmp_b, 8, memTaint, Secret);
    ensures
        let a0 := old(buffer64_read(inA_b, 0, mem));
        let a1 := old(buffer64_read(inA_b, 1, mem));
        let a2 := old(buffer64_read(inA_b, 2, mem));
        let a3 := old(buffer64_read(inA_b, 3, mem));

        let d0 := buffer64_read(dst_b, 0, mem);
        let d1 := buffer64_read(dst_b, 1, mem);
        let d2 := buffer64_read(dst_b, 2, mem);
        let d3 := buffer64_read(dst_b, 3, mem);

        let a := pow2_four(a0, a1, a2, a3);
        let d := pow2_four(d0, d1, d2, d3);

        d % prime == old(a * a) % prime;

        //////////////////////////////////////
        //   Framing
        //////////////////////////////////////

        modifies_buffer_2(dst_b, tmp_b, old(mem), mem);

{
    Mov64(rbx, dst_ptr); // Save dst_ptr that will be clobbered by fast_sqr
    fast_sqr(0, tmp_b, inA_b);
    Mov64(inA_ptr, tmp_ptr);
    Mov64(rdi, rbx); // Restore dst_ptr
    carry_wide(0, dst_b, tmp_b);
}


procedure{:quick}{:public}{:exportSpecs} fsqr_stdcall(
    inline win:bool,
    ghost tmp_b:buffer64,
    ghost inA_b:buffer64,
    ghost dst_b:buffer64)
    lets
        tmp_ptr @= rdi; inA_ptr @= rsi; dst_ptr @= rdx; 
        tmp_in := (if win then rcx else rdi);
        inA_in := (if win then rdx else rsi);
        dst_in := (if win then r8  else rdx);
    reads memTaint;
    modifies
        rax; rbx; rcx; rdx; rsi; rdi; rbp; rsp; r8; r9; r10; r11; r12; r13; r14; r15;
        efl; mem; stack;
    requires
        rsp == init_rsp(stack);
        adx_enabled && bmi2_enabled;
        buffers_disjoint(dst_b, inA_b) || dst_b == inA_b;
        buffers_disjoint(dst_b, tmp_b) || dst_b == tmp_b;

        buffers_disjoint(tmp_b, inA_b);

        validDstAddrs64(mem, dst_in, dst_b, 4, memTaint, Secret);
        validSrcAddrs64(mem, inA_in, inA_b, 4, memTaint, Secret);
        validDstAddrs64(mem, tmp_in, tmp_b, 8, memTaint, Secret);
    ensures
        let a0 := old(buffer64_read(inA_b, 0, mem));
        let a1 := old(buffer64_read(inA_b, 1, mem));
        let a2 := old(buffer64_read(inA_b, 2, mem));
        let a3 := old(buffer64_read(inA_b, 3, mem));

        let d0 := buffer64_read(dst_b, 0, mem);
        let d1 := buffer64_read(dst_b, 1, mem);
        let d2 := buffer64_read(dst_b, 2, mem);
        let d3 := buffer64_read(dst_b, 3, mem);

        let a := pow2_four(a0, a1, a2, a3);
        let d := pow2_four(d0, d1, d2, d3);

        d % prime == old(a * a) % prime;

        //////////////////////////////////////
        //   Framing
        //////////////////////////////////////

        modifies_buffer_2(dst_b, tmp_b, old(mem), mem);
        validSrcAddrs64(mem, dst_in, dst_b, 4, memTaint, Secret);

        win ==>  rbx == old(rbx);
        win ==>  rbp == old(rbp);
        win ==>  rdi == old(rdi);
        win ==>  rsi == old(rsi);
        win ==>  rsp == old(rsp);
        win ==>  r12 == old(r12);
        win ==>  r13 == old(r13);
        win ==>  r14 == old(r14);
        win ==>  r15 == old(r15);
        !win ==>  rbx == old(rbx);
        !win ==>  rbp == old(rbp);
        !win ==>  r12 == old(r12);
        !win ==>  r13 == old(r13);
        !win ==>  r14 == old(r14);
        !win ==>  r15 == old(r15);

        rsp == old(rsp);
{
    Push(r15);
    Push(r12);
    Push(r13);
    Push(r14);
    Push(rbx);

    // Line up the rest of the arguments
    inline if (win) {
        // Store callee-save registers
        Push(rsi);
        Push(rdi);

        Mov64(tmp_ptr, rcx);
        Mov64(inA_ptr, rdx);
        Mov64(dst_ptr, r8);
    }

    fsqr(tmp_b, inA_b, dst_b);

    inline if (win) {
        Pop(rdi);
        Pop(rsi);
    } 

    Pop(rbx);
    Pop(r14);
    Pop(r13);
    Pop(r12);
    Pop(r15);
}

procedure{:quick}{:public}{:exportSpecs} fsqr2(
    ghost tmp_b:buffer64,
    ghost inA_b:buffer64,
    ghost dst_b:buffer64)
    lets
        tmp_ptr @= rdi; inA_ptr @= rsi; dst_ptr @= rdx; 
        tmp_in := rdi;
        inA_in := rsi;
        dst_in := rdx;
    reads memTaint;
    modifies
        rax; rbx; rcx; rdx; rdi; rsi; r8; r9; r10; r11; r12; r13; r14; r15;         
        efl; mem;    
    requires
        adx_enabled && bmi2_enabled;
        buffers_disjoint(dst_b, inA_b) || dst_b == inA_b;
        buffers_disjoint(dst_b, tmp_b) || dst_b == tmp_b;

        buffers_disjoint(tmp_b, inA_b);

        validDstAddrs64(mem, dst_in, dst_b, 8, memTaint, Secret);
        validSrcAddrs64(mem, inA_in, inA_b, 8, memTaint, Secret);
        validDstAddrs64(mem, tmp_in, tmp_b, 16, memTaint, Secret);
    ensures
        let a0  := old(buffer64_read(inA_b, 0, mem));
        let a1  := old(buffer64_read(inA_b, 1, mem));
        let a2  := old(buffer64_read(inA_b, 2, mem));
        let a3  := old(buffer64_read(inA_b, 3, mem));
        let a0' := old(buffer64_read(inA_b, 0 + 4, mem));
        let a1' := old(buffer64_read(inA_b, 1 + 4, mem));
        let a2' := old(buffer64_read(inA_b, 2 + 4, mem));
        let a3' := old(buffer64_read(inA_b, 3 + 4, mem));

        let d0  := buffer64_read(dst_b, 0, mem);
        let d1  := buffer64_read(dst_b, 1, mem);
        let d2  := buffer64_read(dst_b, 2, mem);
        let d3  := buffer64_read(dst_b, 3, mem);
        let d0' := buffer64_read(dst_b, 0 + 4, mem);
        let d1' := buffer64_read(dst_b, 1 + 4, mem);
        let d2' := buffer64_read(dst_b, 2 + 4, mem);
        let d3' := buffer64_read(dst_b, 3 + 4, mem);

        let a  := pow2_four(a0,  a1,  a2,  a3);
        let a' := pow2_four(a0', a1', a2', a3');
        let d  := pow2_four(d0,  d1,  d2,  d3);
        let d' := pow2_four(d0', d1', d2', d3');

        d  % prime == old(a * a)   % prime;
        d' % prime == old(a' * a') % prime;

        //////////////////////////////////////
        //   Framing
        //////////////////////////////////////

        modifies_buffer_2(dst_b, tmp_b, old(mem), mem);

{
    Mov64(rbx, dst_ptr); // Save dst_ptr that will be clobbered by fast_sqr
    fast_sqr(0, tmp_b, inA_b);
    fast_sqr(4, tmp_b, inA_b);
    Mov64(inA_ptr, tmp_ptr);
    Mov64(rdi, rbx); // Restore dst_ptr
    carry_wide(0, dst_b, tmp_b);
    carry_wide(4, dst_b, tmp_b);
}


procedure{:quick}{:public}{:exportSpecs} fsqr2_stdcall(
    inline win:bool,
    ghost tmp_b:buffer64,
    ghost inA_b:buffer64,
    ghost dst_b:buffer64)
    lets
        tmp_ptr @= rdi; inA_ptr @= rsi; dst_ptr @= rdx; 
        tmp_in := (if win then rcx else rdi);
        inA_in := (if win then rdx else rsi);
        dst_in := (if win then r8  else rdx);
    reads memTaint;
    modifies
        rax; rbx; rcx; rdx; rsi; rdi; rbp; rsp; r8; r9; r10; r11; r12; r13; r14; r15;
        efl; mem; stack;
    requires
        rsp == init_rsp(stack);
        adx_enabled && bmi2_enabled;
        buffers_disjoint(dst_b, inA_b) || dst_b == inA_b;
        buffers_disjoint(dst_b, tmp_b) || dst_b == tmp_b;

        buffers_disjoint(tmp_b, inA_b);

        validDstAddrs64(mem, dst_in, dst_b, 8, memTaint, Secret);
        validSrcAddrs64(mem, inA_in, inA_b, 8, memTaint, Secret);
        validDstAddrs64(mem, tmp_in, tmp_b, 16, memTaint, Secret);
    ensures
        let a0  := old(buffer64_read(inA_b, 0, mem));
        let a1  := old(buffer64_read(inA_b, 1, mem));
        let a2  := old(buffer64_read(inA_b, 2, mem));
        let a3  := old(buffer64_read(inA_b, 3, mem));
        let a0' := old(buffer64_read(inA_b, 0 + 4, mem));
        let a1' := old(buffer64_read(inA_b, 1 + 4, mem));
        let a2' := old(buffer64_read(inA_b, 2 + 4, mem));
        let a3' := old(buffer64_read(inA_b, 3 + 4, mem));

        let d0  := buffer64_read(dst_b, 0, mem);
        let d1  := buffer64_read(dst_b, 1, mem);
        let d2  := buffer64_read(dst_b, 2, mem);
        let d3  := buffer64_read(dst_b, 3, mem);
        let d0' := buffer64_read(dst_b, 0 + 4, mem);
        let d1' := buffer64_read(dst_b, 1 + 4, mem);
        let d2' := buffer64_read(dst_b, 2 + 4, mem);
        let d3' := buffer64_read(dst_b, 3 + 4, mem);

        let a  := pow2_four(a0,  a1,  a2,  a3);
        let a' := pow2_four(a0', a1', a2', a3');
        let d  := pow2_four(d0,  d1,  d2,  d3);
        let d' := pow2_four(d0', d1', d2', d3');

        d  % prime == old(a * a)   % prime;
        d' % prime == old(a' * a') % prime;

        //////////////////////////////////////
        //   Framing
        //////////////////////////////////////

        modifies_buffer_2(dst_b, tmp_b, old(mem), mem);
        validSrcAddrs64(mem, dst_in, dst_b, 8, memTaint, Secret);

        win ==>  rbx == old(rbx);
        win ==>  rbp == old(rbp);
        win ==>  rdi == old(rdi);
        win ==>  rsi == old(rsi);
        win ==>  rsp == old(rsp);
        win ==>  r12 == old(r12);
        win ==>  r13 == old(r13);
        win ==>  r14 == old(r14);
        win ==>  r15 == old(r15);
        !win ==>  rbx == old(rbx);
        !win ==>  rbp == old(rbp);
        !win ==>  r12 == old(r12);
        !win ==>  r13 == old(r13);
        !win ==>  r14 == old(r14);
        !win ==>  r15 == old(r15);

        rsp == old(rsp);
{
    Push(r15);
    Push(r12);
    Push(r13);
    Push(r14);
    Push(rbx);

    // Line up the rest of the arguments
    inline if (win) {
        // Store callee-save registers
        Push(rsi);
        Push(rdi);

        Mov64(tmp_ptr, rcx);
        Mov64(inA_ptr, rdx);
        Mov64(dst_ptr, r8);
    }

    fsqr2(tmp_b, inA_b, dst_b);

    inline if (win) {
        Pop(rdi);
        Pop(rsi);
    } 

    Pop(rbx);
    Pop(r14);
    Pop(r13);
    Pop(r12);
    Pop(r15);
}


